{
  "name": "elegant-drawerv2",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:production": "tsc -b && vite build --mode production",
    "lint": "eslint .",
    "preview": "vite preview",
    "docker:build": "docker build -t elegant-drawer .",
    "docker:run": "docker run -p 8080:80 elegant-drawer",
    "docker:test": "docker build -t elegant-drawer-test . && docker run -p 8080:80 elegant-drawer-test",
    "deploy:check": "node -e \"console.log('Deployment files ready!')\"",
    "serve": "vite preview --host 0.0.0.0 --port 4173"
  },
  "dependencies": {
    "@tailwindcss/typography": "^0.5.16",
    "@types/d3": "^7.4.3",
    "autoprefixer": "10.4.20",
    "clsx": "^2.1.1",
    "d3": "^7.9.0",
    "konva": "^10.0.2",
    "lucide-react": "^0.544.0",
    "postcss": "^8.5.6",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-konva": "^19.0.10",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "3.4.14",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.2",
    "eslint": "^9.35.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.4.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.43.0",
    "vite": "^7.1.6"
  }
}